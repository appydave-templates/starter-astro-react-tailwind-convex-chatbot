{
  "name": "ai-starter-template",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "convex"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "convex:dev": "cd convex && bun run dev",
    "convex:deploy": "cd convex && bun run deploy"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "wrangler": "^4.23.0"
  },
  "packageManager": "bun@1.1.0"
}